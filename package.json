  "scripts": {
    "chrome": "wdio ./cucumber-config/chrome.js",
    "chrome-headless": "wdio ./cucumber-config/chrome-headless.js",
    "cloud-chrome": "wdio ./cucumber-config/cloud.js",
    "cloud-debug": "wdio ./cucumber-config/cloud-debug.js",
    "view-report": "node ./cucumber-config/allure/view-report.js",
    "view-and-archive-report": "node ./cucumber-config/allure/view-and-archive-report.js",
    "view-archived-report": "node ./cucumber-config/allure/view-archived-report.js",
    "postinstall": "npm run copy-parameters && npm run copy-user-stored-values && npm run create-workspace",
    "copy-parameters": "if not exist cucumber-config\\parameters (copy cucumber-config\\parameters_template cucumber-config\\parameters)",
    "copy-user-stored-values": "if not exist cucumber-user-config\\user\\user-stored-values (copy cucumber-user-config\\user\\user-stored-values_template cucumber-user-config\\user\\user-stored-values)",
    "create-workspace": "if not exist Workspace (mkdir Workspace)",
    "allure-pdf-print": "node ./cucumber-config/allure/allure-to-pdf.js"
  },
